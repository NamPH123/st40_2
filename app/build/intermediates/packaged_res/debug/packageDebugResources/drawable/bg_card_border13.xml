<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape >
            <gradient
                android:angle="-45"
                android:endColor="#CE50E0"
                android:startColor="#FFB951" />
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item android:top="1.5dp" android:start="1.5dp" android:end="1.5dp" android:bottom="1.5dp">
        <shape >
            <solid android:color="@color/black"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
</layer-list>
